	"_moduleAliases": {
		"@": "built"
	},
	"jest": {
		"testRegex": "/test/.*\\.(test|spec)\\.ts$",
		"testPathIgnorePatterns": [
			"/node_modules/",
			"/test/__mocks__/",
			"/test/__modules__/"
		],
		"moduleFileExtensions": [
			"ts",
			"js"
		],
		"transform": {
			"^.+\\.tsx?$": [
				"ts-jest",
				{
					"tsconfig": "test/tsconfig.json"
				}
			]
		},
		"moduleNameMapper": {
			"^@/(.+)": "<rootDir>/src/$1",
			"^#/(.+)": "<rootDir>/test/$1"
		}
	}
